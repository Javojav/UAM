digraph {
  rankdir=LR;

  node [shape = point]; __start_point__
  4[shape=doublecircle]
  1[shape=circle]
  0[shape=circle]
  3[shape=circle]
  2[shape=circle]

  __start_point__ -> 0
  3 -> 3[label="0"]
  1 -> 3[label="0"]
  2 -> 3[label="1"]
  1 -> 4[label="1"]
  3 -> 0[label="1"]
  4 -> 4[label="1"]
  0 -> 2[label="1"]
  0 -> 1[label="0"]
  4 -> 0[label="0"]
  2 -> 4[label="0"]
}
