digraph {
  rankdir=LR;

  node [shape = point]; __start_point__
  4[shape=circle]
  2[shape=doublecircle]
  0[shape=doublecircle]
  3[shape=doublecircle]

  __start_point__ -> 0
  2 -> 0[label="c"]
  0 -> 0[label="c"]
  3 -> 4[label="c"]
  0 -> 2[label="b"]
  4 -> 4[label="a"]
  2 -> 0[label="a"]
  4 -> 4[label="b"]
  4 -> 4[label="c"]
  3 -> 4[label="b"]
  3 -> 4[label="a"]
  2 -> 3[label="b"]
  0 -> 0[label="a"]
}
