digraph {
  rankdir=LR;

  node [shape = point]; __start_point__
  q0[shape=circle]
  q1[shape=doublecircle]
  q2[shape=circle]
  q3[shape=doublecircle]
  q4[shape=circle]
  qx[shape=circle]

  __start_point__ -> q0
  qx -> qx[label="b"]
  q4 -> q3[label="a"]
  q1 -> q2[label="a"]
  q2 -> q2[label="b"]
  q1 -> q1[label="b"]
  q3 -> q4[label="a"]
  q3 -> q3[label="b"]
  q0 -> q1[label="a"]
  q4 -> q4[label="b"]
  qx -> qx[label="a"]
  q2 -> q1[label="a"]
  q0 -> q3[label="b"]
}
