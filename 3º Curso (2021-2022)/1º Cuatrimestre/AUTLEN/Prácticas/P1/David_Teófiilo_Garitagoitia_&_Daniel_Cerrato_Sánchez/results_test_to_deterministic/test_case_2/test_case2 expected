digraph {
  rankdir=LR;

  node [shape = point]; __start_point__
  0[shape=circle]
  1[shape=circle]
  2[shape=doublecircle]
  trash[shape=circle]

  __start_point__ -> 0
  2 -> 2[label="1"]
  trash -> trash[label="1"]
  1 -> 1[label="0"]
  trash -> trash[label="0"]
  2 -> 1[label="0"]
  0 -> trash[label="0"]
  1 -> 2[label="1"]
  0 -> 1[label="1"]
}
