digraph {
  rankdir=LR;

  node [shape = point]; __start_point__
  1[shape=circle]
  Trash[shape=circle]
  0[shape=circle]
  2[shape=doublecircle]

  __start_point__ -> 0
  Trash -> Trash[label="1"]
  0 -> Trash[label="0"]
  2 -> 2[label="1"]
  1 -> 1[label="0"]
  Trash -> Trash[label="0"]
  2 -> 1[label="0"]
  1 -> 2[label="1"]
  0 -> 1[label="1"]
}
